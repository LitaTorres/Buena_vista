<%= form_with(model: state) do |form| %>
  <% if state.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(state.errors.count, "error") %> prohibited this state from being saved:</h2>

      <ul>
        <% state.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :available, style: "display: block" %>
    <%= form.text_field :available %>
  </div>

  <div>
    <%= form.label :rent, style: "display: block" %>
    <%= form.text_field :rent %>
  </div>

  <div>
    <%= form.label :sale, style: "display: block" %>
    <%= form.text_field :sale %>
  </div>

  <div>
    <%= form.label :leased, style: "display: block" %>
    <%= form.text_field :leased %>
  </div>

  <div>
    <%= form.label :sold, style: "display: block" %>
    <%= form.text_field :sold %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
